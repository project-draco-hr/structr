{
  Class type=null;
  String id=null;
  for (  Object obj : parameters) {
    if (obj instanceof Class) {
      type=(Class)obj;
    }
 else     if (obj instanceof String) {
      id=(String)obj;
    }
  }
  try {
    return StructrObject.dbGet(type,id);
  }
 catch (  Throwable t) {
    publishProgress(new Progress(t));
  }
  return null;
}
