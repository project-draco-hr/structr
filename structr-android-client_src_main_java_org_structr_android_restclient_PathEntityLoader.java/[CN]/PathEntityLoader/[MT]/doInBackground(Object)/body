{
  StringBuilder path=new StringBuilder();
  Class type=null;
  for (  Object obj : parameters) {
    if (obj instanceof Class) {
      type=(Class)obj;
    }
 else {
      path.append(obj.toString());
    }
  }
  try {
    return StructrObject.dbLoad(type,path.toString());
  }
 catch (  Throwable t) {
    publishProgress(new Progress(t));
  }
  return null;
}
