{
  StringBuilder result=new StringBuilder();
  int[] eatLength=new int[1];
  while (s.length() != 0) {
    char c=eatPercentage(s,eatLength);
    if (eatLength[0] > 1) {
      result.append(c);
      s=s.substring(eatLength[0]);
    }
 else {
      result.append(s.charAt(0));
      s=s.substring(1);
    }
  }
  return (result.toString());
}
