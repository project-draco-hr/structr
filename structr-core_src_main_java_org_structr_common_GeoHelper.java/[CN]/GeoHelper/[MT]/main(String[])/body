{
  String address="Hanauer Landstr. 291a";
  try {
    GeoCodingResult res=geocode(address);
    logger.log(Level.INFO,"result for address {0}: {1} ({2})",new Object[]{address,res.getLatitude(),res.getAddressComponent(Type.administrative_area_level_1).getLongValue()});
  }
 catch (  Exception ex) {
    logger.log(Level.SEVERE,null,ex);
  }
}
