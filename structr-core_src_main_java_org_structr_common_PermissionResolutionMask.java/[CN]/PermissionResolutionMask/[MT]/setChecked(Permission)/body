{
  if (Permission.read.equals(permission)) {
    checked|=READ;
  }
  if (Permission.write.equals(permission)) {
    checked|=WRITE;
  }
  if (Permission.delete.equals(permission)) {
    checked|=DELETE;
  }
  if (Permission.accessControl.equals(permission)) {
    checked|=ACCESS_CONTROL;
  }
  return false;
}
