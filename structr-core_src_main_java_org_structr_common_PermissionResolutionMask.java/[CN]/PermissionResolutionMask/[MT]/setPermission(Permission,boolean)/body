{
  if (Permission.read.equals(permission)) {
    if (isAllowed) {
      value|=READ;
    }
 else {
      value^=READ;
    }
  }
  if (Permission.write.equals(permission)) {
    if (isAllowed) {
      value|=WRITE;
    }
 else {
      value^=WRITE;
    }
  }
  if (Permission.delete.equals(permission)) {
    if (isAllowed) {
      value|=DELETE;
    }
 else {
      value^=DELETE;
    }
  }
  if (Permission.accessControl.equals(permission)) {
    if (isAllowed) {
      value|=ACCESS_CONTROL;
    }
 else {
      value^=ACCESS_CONTROL;
    }
  }
  return false;
}
