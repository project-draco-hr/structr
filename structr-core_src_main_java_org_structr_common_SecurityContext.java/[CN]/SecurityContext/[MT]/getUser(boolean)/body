{
  if (cachedUser != null) {
    return cachedUser;
  }
  if (authenticator == null) {
    return null;
  }
  if (authenticator.hasExaminedRequest()) {
    return null;
  }
  try {
    cachedUser=authenticator.getUser(request,tryLogin);
  }
 catch (  Throwable t) {
    logger.log(Level.WARNING,"No user found");
  }
  return cachedUser;
}
