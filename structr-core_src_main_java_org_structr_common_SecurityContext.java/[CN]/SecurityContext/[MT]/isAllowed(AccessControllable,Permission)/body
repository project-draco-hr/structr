{
  if (node == null) {
    return false;
  }
  if (isSuperUser()) {
    return true;
  }
  Principal user=getUser();
  if (user == null) {
    return false;
  }
  if ((user != null) && (user.equals(node) || user.equals(node.getOwnerNode()))) {
    return true;
  }
  boolean isAllowed=false;
switch (accessMode) {
case Backend:
    isAllowed=isAllowedInBackend(node,permission);
  break;
case Frontend:
isAllowed=isAllowedInFrontend(node,permission);
break;
}
logger.log(Level.FINEST,"Returning {0} for user {1}, access mode {2}, node {3}, permission {4}",new Object[]{isAllowed,(user != null) ? user.getProperty(AbstractNode.name) : "null",accessMode,node,permission});
return isAllowed;
}
