{
  final StringBuilder buf=new StringBuilder();
  if (type != null) {
    buf.append(type);
  }
  if (property != null) {
    buf.append(".");
    buf.append(property);
  }
  if (token != null) {
    buf.append(" ");
    buf.append(token);
  }
  if (detail != null) {
    buf.append(" ");
    buf.append(detail);
  }
  return buf.toString();
}
