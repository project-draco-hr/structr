{
  Set<Method> methods=new LinkedHashSet<Method>();
  Set<Class<?>> allTypes=getAllTypes(entityType);
  for (  Class<?> type : allTypes) {
    for (    Method method : type.getDeclaredMethods()) {
      if (method.getAnnotation(annotationType) != null) {
        methods.add(method);
      }
    }
  }
  return methods;
}
