{
  if (p.isNumber()) {
    Number number=p.getAsNumber();
    if (number instanceof Integer) {
      return number.intValue();
    }
 else     if (number instanceof Long) {
      return number.longValue();
    }
 else {
      return number.doubleValue();
    }
  }
 else   if (p.isBoolean()) {
    return p.getAsBoolean();
  }
  return p.getAsString();
}
