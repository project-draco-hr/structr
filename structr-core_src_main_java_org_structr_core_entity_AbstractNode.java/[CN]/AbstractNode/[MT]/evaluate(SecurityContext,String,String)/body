{
switch (key) {
case "owner":
    return getOwnerNode();
default :
  Object value=getProperty(StructrApp.getConfiguration().getPropertyKeyForJSONName(entityType,key));
if (value != null) {
  return value;
}
value=invokeMethod(key,Collections.EMPTY_MAP);
if (value != null) {
return value;
}
return Functions.numberOrString(defaultValue);
}
}
