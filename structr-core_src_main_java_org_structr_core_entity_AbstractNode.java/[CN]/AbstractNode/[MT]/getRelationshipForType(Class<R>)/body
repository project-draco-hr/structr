{
  R instance=(R)relationshipTemplateInstanceCache.get(type.getName());
  if (instance == null) {
    try {
      instance=type.newInstance();
      relationshipTemplateInstanceCache.put(type.getName(),instance);
    }
 catch (    Throwable t) {
      logger.log(Level.WARNING,"",t);
    }
  }
  return instance;
}
