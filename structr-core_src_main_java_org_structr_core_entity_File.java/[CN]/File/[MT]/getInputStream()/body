{
  URL url=null;
  InputStream in=null;
  try {
    url=getFileLocation();
    return url.openStream();
  }
 catch (  IOException e) {
    logger.log(Level.SEVERE,"Error while reading from {0}",new Object[]{url,e.getMessage()});
    if (in != null) {
      try {
        in.close();
      }
 catch (      IOException ignore) {
      }
    }
  }
  return null;
}
