{
  Iterable<AbstractRelationship> rels=child.getIncomingRelationships(relType);
  if (rels != null && rels.iterator().hasNext()) {
    AbstractRelationship rel=rels.iterator().next();
    Integer pos=rel.getProperty(positionProperty);
    if (pos != null) {
      return pos.intValue();
    }
  }
  return -1;
}
