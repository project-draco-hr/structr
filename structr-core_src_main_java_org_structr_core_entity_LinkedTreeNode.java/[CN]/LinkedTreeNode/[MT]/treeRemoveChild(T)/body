{
  final App app=StructrApp.getInstance(securityContext);
  try {
    app.beginTx();
    LinkedTreeNode.super.listRemove(childToRemove);
    unlinkNodes(getChildLinkType(),(T)LinkedTreeNode.this,childToRemove);
    ensureCorrectChildPositions();
    app.commitTx();
  }
  finally {
    app.finishTx();
  }
}
