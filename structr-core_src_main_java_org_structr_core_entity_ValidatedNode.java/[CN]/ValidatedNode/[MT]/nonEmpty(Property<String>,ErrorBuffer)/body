{
  String value=getProperty(property);
  boolean valid=true;
  if (value == null) {
    errorBuffer.add(getClass().getSimpleName(),new NullPropertyToken(property));
    valid=false;
  }
 else   if (value.isEmpty()) {
    errorBuffer.add(getClass().getSimpleName(),new EmptyPropertyToken(property));
    valid=false;
  }
  return valid;
}
