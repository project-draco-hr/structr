{
  final List list=new ArrayList();
  for (  final Object source : sources) {
    if (source instanceof Collection) {
      for (      Object obj : (Collection)source) {
        if (obj != null) {
          list.add(obj);
        }
      }
    }
 else     if (source != null) {
      list.add(source);
    }
  }
  return list;
}
