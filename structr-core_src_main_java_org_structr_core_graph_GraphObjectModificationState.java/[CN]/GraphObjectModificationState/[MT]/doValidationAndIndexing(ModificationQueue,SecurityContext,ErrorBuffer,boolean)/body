{
  boolean valid=true;
switch (status & 0x000f) {
case 6:
    if (doValidation) {
      valid&=validate(securityContext,errorBuffer);
    }
  object.indexPassiveProperties();
break;
case 4:
if (doValidation) {
valid&=validate(securityContext,errorBuffer);
}
object.indexPassiveProperties();
break;
case 2:
if (doValidation) {
valid&=validate(securityContext,errorBuffer);
}
object.indexPassiveProperties();
break;
case 1:
object.removeFromIndex();
break;
default :
break;
}
return valid;
}
