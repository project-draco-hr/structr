{
  boolean valid=true;
switch (status & 0x000f) {
case 6:
    if (doValidation) {
      valid&=validate(securityContext,errorBuffer);
    }
  addToIndex();
break;
case 4:
if (doValidation) {
valid&=validate(securityContext,errorBuffer);
}
addToIndex();
break;
case 2:
if (doValidation) {
valid&=validate(securityContext,errorBuffer);
}
updateInIndex();
break;
default :
break;
}
return valid;
}
