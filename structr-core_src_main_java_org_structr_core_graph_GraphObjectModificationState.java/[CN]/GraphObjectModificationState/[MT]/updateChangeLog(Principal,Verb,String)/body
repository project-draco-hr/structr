{
  if (auditLogEnabled && changeLog != null) {
    changeLog.append(System.currentTimeMillis());
    changeLog.append(":");
    changeLog.append(user.getUuid());
    changeLog.append(":");
    changeLog.append(user.getName());
    changeLog.append(":");
    changeLog.append(verb);
    changeLog.append(":");
    changeLog.append(object);
    changeLog.append("\n");
  }
}
