{
  final App app=StructrApp.getInstance();
  try (final Tx tx=app.tx()){
    exportToStream(new FileOutputStream(fileName),app.nodeQuery(NodeInterface.class).includeDeletedAndHidden().getAsList(),app.relationshipQuery(RelationshipInterface.class).includeDeletedAndHidden().getAsList(),null,includeFiles);
    tx.success();
  }
 catch (  Throwable t) {
    throw new FrameworkException(500,t.getMessage());
  }
}
