{
  final TransactionReference tx=transactions.get();
  if (tx != null) {
    if (tx.isToplevel()) {
      tx.finish();
      currentCommand.remove();
      transactions.remove();
      modificationQueue.doOuterCallbacks(securityContext);
      modificationQueue.clear();
    }
 else {
      tx.end();
    }
  }
}
