{
  Set<Semaphore> semaphores=new LinkedHashSet<Semaphore>();
  if (types != null) {
    for (    String type : types) {
      if (type != null) {
        Semaphore semaphore=semaphoreMap.get(type);
        if (semaphore == null) {
          semaphore=new Semaphore(1,true);
          semaphoreMap.put(type,semaphore);
        }
        semaphores.add(semaphore);
      }
    }
  }
  return semaphores;
}
