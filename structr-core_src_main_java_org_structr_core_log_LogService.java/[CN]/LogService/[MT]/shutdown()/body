{
  if (isRunning()) {
    for (    RunnableService s : registeredServices) {
      s.stopService();
    }
    waitFor(registeredServices.isEmpty());
    try {
      logDbFactory.close();
    }
 catch (    IOException ex) {
      logger.log(Level.SEVERE,"Log database was not closed properly",ex);
    }
    logDbFactory=null;
    isInitialized=false;
  }
}
