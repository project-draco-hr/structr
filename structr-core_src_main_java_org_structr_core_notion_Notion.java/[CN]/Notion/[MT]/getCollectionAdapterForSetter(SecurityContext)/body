{
  return new Adapter<List<T>,List<S>>(){
    @Override public List<S> adapt(    List<T> s) throws FrameworkException {
      List<S> list=new LinkedList<S>();
      for (      T t : s) {
        list.add(deserializationStrategy.deserialize(securityContext,type,t));
      }
      return list;
    }
  }
;
}
