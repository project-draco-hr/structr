{
  if (obj instanceof Date) {
    return Double.valueOf(((Date)obj).getTime());
  }
 else   if (obj instanceof Number) {
    return ((Number)obj).doubleValue();
  }
 else {
    try {
      return Double.valueOf(obj.toString());
    }
 catch (    Throwable t) {
      t.printStackTrace();
    }
  }
  return null;
}
