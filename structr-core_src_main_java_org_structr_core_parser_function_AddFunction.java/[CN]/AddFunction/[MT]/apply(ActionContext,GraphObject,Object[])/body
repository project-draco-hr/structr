{
  Double result=0.0;
  if (sources != null) {
    for (    Object i : sources) {
      if (i != null) {
        try {
          result+=Double.parseDouble(i.toString());
        }
 catch (        Throwable t) {
          logException(t,sources);
          return t.getMessage();
        }
      }
    }
  }
  return result;
}
