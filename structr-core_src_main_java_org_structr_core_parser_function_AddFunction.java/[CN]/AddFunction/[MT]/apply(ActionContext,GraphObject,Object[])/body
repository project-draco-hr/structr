{
  Double result=0.0;
  if (sources != null) {
    for (    Object i : sources) {
      if (i != null) {
        try {
          result+=Double.parseDouble(i.toString());
        }
 catch (        Throwable t) {
          return t.getMessage();
        }
      }
 else {
        result+=0.0;
      }
    }
  }
  return result;
}
