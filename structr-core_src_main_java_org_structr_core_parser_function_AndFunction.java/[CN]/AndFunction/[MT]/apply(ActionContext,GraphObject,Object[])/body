{
  boolean result=true;
  if (arrayHasMinLengthAndAllElementsNotNull(sources,2)) {
    for (    Object i : sources) {
      if (i != null) {
        try {
          result&="true".equals(i.toString()) || Boolean.TRUE.equals(i);
        }
 catch (        Throwable t) {
          logException(t,sources);
          return t.getMessage();
        }
      }
 else {
        return false;
      }
    }
  }
 else {
    logParameterError(sources,ctx.isJavaScriptContext());
    result=false;
  }
  return result;
}
