{
  if (arrayHasMinLengthAndAllElementsNotNull(sources,1)) {
    final int level=parseInt(sources[0]);
    ctx.incrementCounter(level);
    if (sources.length == 2 && "true".equals(sources[1].toString())) {
      for (int i=level + 1; i < 10; i++) {
        ctx.resetCounter(i);
      }
    }
  }
  return "";
}
