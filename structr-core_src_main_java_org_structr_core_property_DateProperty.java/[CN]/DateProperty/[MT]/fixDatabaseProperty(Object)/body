{
  if (value != null) {
    if (value instanceof Long) {
      return value;
    }
    if (value instanceof Number) {
      return ((Number)value).longValue();
    }
    try {
      return Long.parseLong(value.toString());
    }
 catch (    Throwable t) {
    }
    try {
      return new SimpleDateFormat(format).parse(value.toString()).getTime();
    }
 catch (    Throwable t) {
    }
  }
  return null;
}
