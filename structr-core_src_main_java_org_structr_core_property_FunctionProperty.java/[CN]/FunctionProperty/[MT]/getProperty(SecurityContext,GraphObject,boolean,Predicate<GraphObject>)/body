{
  if (obj instanceof AbstractNode) {
    try {
      return (T)Scripting.evaluate(new ActionContext(securityContext),obj,"${".concat(format).concat("}"));
    }
 catch (    Throwable t) {
      logger.log(Level.WARNING,"Exception while evaluating function property '{0}'.",jsonName());
      t.printStackTrace();
    }
  }
  return null;
}
