{
  if (StringUtils.isNotBlank(source)) {
    Date result=DatePropertyParser.parseISO8601DateString(source);
    if (result != null) {
      return result;
    }
 else {
      throw new FrameworkException(422,new DateFormatToken(declaringClass.getSimpleName(),ISO8601DateProperty.this));
    }
  }
  return null;
}
