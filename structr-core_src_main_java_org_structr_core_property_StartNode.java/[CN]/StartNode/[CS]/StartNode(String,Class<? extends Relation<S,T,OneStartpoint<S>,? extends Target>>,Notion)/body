{
  super(name);
  try {
    this.relation=relationClass.newInstance();
  }
 catch (  Throwable t) {
    t.printStackTrace();
  }
  this.notion=notion;
  this.destType=relation.getTargetType();
  this.relationClass=relationClass;
  this.notion.setType(destType);
  EntityContext.registerConvertedProperty(this);
}
