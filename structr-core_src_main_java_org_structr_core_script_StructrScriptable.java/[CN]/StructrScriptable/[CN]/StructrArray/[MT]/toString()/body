{
  final StringBuilder buf=new StringBuilder();
  boolean first=true;
  for (  final Object obj : (Object[])unwrap()) {
    if (first) {
      buf.append("[");
      first=false;
    }
 else {
      buf.append(",");
    }
    buf.append(obj.toString());
  }
  buf.append("]");
  return buf.toString();
}
