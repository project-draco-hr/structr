{
  if (source != null) {
    if (source instanceof Wrapper) {
      return unwrap(((Wrapper)source).unwrap());
    }
 else {
      if (source.getClass().isArray()) {
        final List list=new ArrayList();
        for (        final Object obj : (Object[])source) {
          list.add(unwrap(obj));
        }
        return list;
      }
    }
  }
  return source;
}
