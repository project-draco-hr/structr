{
  final App app=StructrApp.getInstance();
  try {
    app.beginTx();
    for (    DynamicResourceAccess grant : app.nodeQuery(DynamicResourceAccess.class).getAsList()) {
      app.delete(grant);
    }
    app.commitTx();
  }
 catch (  Throwable t) {
    t.printStackTrace();
  }
 finally {
    app.finishTx();
  }
}
