{
  if (array == null) {
    return false;
  }
  if (minLength != null) {
    if (array.length < minLength) {
      throw new IllegalArgumentException();
    }
    for (    final Object element : array) {
      if (element == null) {
        return false;
      }
    }
    return true;
  }
  return false;
}
