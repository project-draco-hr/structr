{
  return app.create(SchemaProperty.class,new NodeAttribute(SchemaProperty.name,getName()),new NodeAttribute(SchemaProperty.schemaNode,schemaNode),new NodeAttribute(SchemaProperty.unique,isUnique()),new NodeAttribute(SchemaProperty.indexed,isIndexed()),new NodeAttribute(SchemaProperty.notNull,isRequired()),new NodeAttribute(SchemaProperty.defaultValue,defaultValue));
}
