{
  final Object idValue=source.get(JsonSchema.KEY_ID);
  URI id=null;
  if (idValue != null) {
    try {
      id=new URI(idValue.toString());
    }
 catch (    URISyntaxException ex) {
      ex.printStackTrace();
    }
  }
  final StructrSchemaDefinition schema=new StructrSchemaDefinition(id);
  schema.deserialize(source);
  return schema;
}
