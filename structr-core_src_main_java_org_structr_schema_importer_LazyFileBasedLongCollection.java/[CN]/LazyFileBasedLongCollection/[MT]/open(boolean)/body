{
  try {
    if (dos != null) {
      dos.close();
    }
  }
 catch (  IOException ioex) {
    logger.log(Level.WARNING,"",ioex);
  }
  try {
    dos=new DataOutputStream(new BufferedOutputStream(new FileOutputStream(file,append)));
  }
 catch (  IOException ioex) {
    logger.log(Level.WARNING,"",ioex);
