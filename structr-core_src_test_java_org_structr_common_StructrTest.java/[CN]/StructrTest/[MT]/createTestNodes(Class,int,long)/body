{
  try (final Tx tx=app.tx()){
    List<NodeInterface> nodes=new LinkedList<>();
    for (int i=0; i < number; i++) {
      nodes.add(app.create(type));
      try {
        Thread.sleep(delay);
      }
 catch (      InterruptedException ex) {
      }
    }
    tx.success();
    return nodes;
  }
 catch (  Throwable t) {
    t.printStackTrace();
  }
  return null;
}
