{
  final PropertyMap props=new PropertyMap();
  props.put(AbstractNode.type,type.getSimpleName());
  return transactionCommand.execute(new StructrTransaction<List<NodeInterface>>(){
    @Override public List<NodeInterface> execute() throws FrameworkException {
      List<NodeInterface> nodes=new LinkedList<NodeInterface>();
      for (int i=0; i < number; i++) {
        nodes.add(createNodeCommand.execute(props));
        try {
          Thread.sleep(delay);
        }
 catch (        InterruptedException ex) {
        }
      }
      return nodes;
    }
  }
);
}
