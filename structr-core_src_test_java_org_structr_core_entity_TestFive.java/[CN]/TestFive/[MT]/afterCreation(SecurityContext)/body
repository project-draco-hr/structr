{
  final App app=StructrApp.getInstance(securityContext);
  try {
    final int value=getIncreasedValue(modifiedInAfterCreation);
    app.beginTx();
    setProperty(modifiedInAfterCreation,value);
    app.commitTx();
  }
 catch (  Throwable t) {
    t.printStackTrace();
  }
 finally {
    app.finishTx();
  }
}
