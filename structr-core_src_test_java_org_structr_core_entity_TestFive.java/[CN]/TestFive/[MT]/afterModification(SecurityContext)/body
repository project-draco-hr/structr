{
  final App app=StructrApp.getInstance(securityContext);
  try {
    final int value=getIncreasedValue(modifiedInAfterModification);
    app.beginTx();
    setProperty(modifiedInAfterModification,value);
    app.commitTx();
  }
 catch (  Throwable t) {
    t.printStackTrace();
  }
 finally {
    app.finishTx();
  }
}
