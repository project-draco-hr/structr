{
  try {
    final PropertyMap properties=new PropertyMap();
    final PropertyKey<Boolean> key=TestFour.booleanProperty;
    properties.put(key,true);
    final TestFour testEntity=createTestNode(TestFour.class,properties);
    assertNotNull(testEntity);
    assertEquals((Boolean)true,(Boolean)testEntity.getProperty(key));
    Result<TestFour> result=Services.command(securityContext,SearchNodeCommand.class).execute(Search.andExactType(TestFour.class),Search.andExactProperty(securityContext,key,true));
    assertEquals(result.size(),1);
    assertEquals(result.get(0),testEntity);
  }
 catch (  FrameworkException fex) {
    fail("Unable to store array");
  }
}
