{
  if (tx != null) {
    try {
      logDebug("######################## COMMITING TRANSACTION " + tx + " in thread "+ Thread.currentThread(),null);
      tx.success();
    }
 catch (    Throwable t) {
      logger.log(Level.WARNING,"",t);
    }
  }
 else {
    System.out.println("NO TRANSACTION!");
  }
}
