{
  final StructrShellCommand cmd=new StructrShellCommand(){
    @Override public void start(    final Environment env) throws IOException {
      super.start(env);
      this.handleLine(command);
      this.flush();
      this.handleExit();
    }
    @Override public boolean isInteractive(){
      return false;
    }
  }
;
  return cmd;
}
