{
  for (  final char c : line.toCharArray()) {
switch (c) {
case '{':
      inBlock++;
    break;
case '}':
  inBlock--;
break;
case '[':
inArray++;
break;
case ']':
inArray--;
break;
case '(':
inBraces++;
break;
case ')':
inBraces--;
break;
case '"':
inDoubleQuotes++;
break;
case '\'':
inSingleQuotes++;
break;
}
}
}
