{
  if (tabCount == 1 && !insideOfBlockOrStructure()) {
    String mode=null;
switch (consoleMode) {
case 0:
      mode="javascript";
    consoleMode=1;
  break;
case 1:
mode="structr";
consoleMode=2;
break;
case 2:
mode="cypher";
consoleMode=3;
break;
case 3:
mode="shell";
consoleMode=0;
break;
}
term.handleString("Console.setMode('" + mode + "')");
term.clearTabCount();
term.setBold(true);
term.setTextColor(3);
term.handleNewline();
}
}
