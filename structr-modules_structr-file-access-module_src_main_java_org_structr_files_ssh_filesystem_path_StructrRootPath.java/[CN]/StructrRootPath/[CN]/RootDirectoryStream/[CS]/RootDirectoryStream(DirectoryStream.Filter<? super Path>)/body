{
  for (  final StructrPath p : rootPaths.values()) {
    if (p.equals(StructrRootPath.this)) {
      continue;
    }
    try {
      if (filter == null || filter.accept(p)) {
        this.paths.add(p);
      }
    }
 catch (    IOException ioex) {
      logger.log(Level.WARNING,"Unable to create root directory stream",ioex);
    }
  }
}
