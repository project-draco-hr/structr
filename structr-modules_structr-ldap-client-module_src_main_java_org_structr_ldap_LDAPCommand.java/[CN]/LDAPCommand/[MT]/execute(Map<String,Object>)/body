{
  final LDAPService ldapService=Services.getInstance().getService(LDAPService.class);
  if (ldapService != null) {
    try {
      ldapService.doUpdate();
    }
 catch (    Throwable t) {
      Logger.getLogger(LDAPCommand.class.getName()).log(Level.WARNING,"Unable to update LDAP information.",t);
    }
  }
}
