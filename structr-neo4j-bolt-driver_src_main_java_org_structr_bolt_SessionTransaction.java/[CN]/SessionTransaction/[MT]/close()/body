{
  if (!success) {
    for (    final EntityWrapper entity : modifiedEntities) {
      entity.stale();
    }
  }
  closed=true;
  try {
    tx.close();
    session.close();
  }
 catch (  TransientException tex) {
    throw new RetryException(tex);
  }
 finally {
    if (session.isOpen()) {
      session.close();
    }
  }
}
