{
  try {
    if (db.logQueries()) {
      System.out.println(statement);
    }
    return tx.run(statement,map).next().get(0).asEntity();
  }
 catch (  TransientException tex) {
    closed=true;
    throw new RetryException(tex);
  }
catch (  NoSuchRecordException nex) {
    throw new NotFoundException(nex);
  }
}
