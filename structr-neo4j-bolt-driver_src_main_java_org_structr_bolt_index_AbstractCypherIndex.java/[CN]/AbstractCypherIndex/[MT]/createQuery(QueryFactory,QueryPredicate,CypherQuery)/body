{
  final Class type=predicate.getQueryType();
  if (type != null) {
    final QueryFactory factory=FACTORIES.get(type);
    if (factory != null) {
      factory.createQuery(this,predicate,query);
    }
 else {
      logger.log(Level.WARNING,"No query factory registered for type {0}",type);
    }
  }
 else {
    query.noop();
  }
}
