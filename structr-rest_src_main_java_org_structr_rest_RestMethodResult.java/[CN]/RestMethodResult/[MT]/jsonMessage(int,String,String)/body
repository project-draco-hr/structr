{
  StringBuilder buf=new StringBuilder(100);
  buf.append("{\n");
  buf.append("  \"code\" : ").append(code);
  if (message != null) {
    buf.append(",\n  \"").append(messageKey).append("\" : \"").append(org.apache.commons.lang.StringUtils.replace(message,"\"","\\\"")).append("\"\n");
  }
 else {
    buf.append("\n");
  }
  buf.append("}\n");
  return buf.toString();
}
