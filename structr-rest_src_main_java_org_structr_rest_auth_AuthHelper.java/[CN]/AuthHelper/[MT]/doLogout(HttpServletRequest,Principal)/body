{
  final HttpSession session=request.getSession(false);
  if (session != null) {
    SessionHelper.clearSession(session.getId());
    SessionHelper.invalidateSession(session);
    Actions.call(Actions.NOTIFICATION_LOGOUT,user);
    try {
      request.logout();
      request.changeSessionId();
    }
 catch (    Throwable t) {
    }
  }
}
