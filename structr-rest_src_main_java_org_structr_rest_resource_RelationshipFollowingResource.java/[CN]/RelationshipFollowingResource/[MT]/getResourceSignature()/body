{
  StringBuilder uri=new StringBuilder();
  for (  String part : uriParts) {
    if (part.contains("/")) {
      String[] parts=StringUtils.split(part,"/");
      for (      String subPart : parts) {
        if (!subPart.matches("[a-zA-Z0-9]{32}")) {
          uri.append(subPart);
          uri.append("/");
        }
      }
    }
 else {
      if (!part.matches("[a-zA-Z0-9]{32}")) {
        uri.append(part);
        uri.append("/");
      }
    }
  }
  return uri.toString();
}
