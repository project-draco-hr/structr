{
  if ("id".equals(lastName)) {
    beginTag("a",false,new Css("id"),new Href(value));
    writer.print(value);
    endTag("a",false);
  }
 else {
    beginTag("span",false,new Css("string"));
    writer.print(value);
    endTag("span",false);
  }
  endTag(LI,true);
  hasName=false;
  return this;
}
