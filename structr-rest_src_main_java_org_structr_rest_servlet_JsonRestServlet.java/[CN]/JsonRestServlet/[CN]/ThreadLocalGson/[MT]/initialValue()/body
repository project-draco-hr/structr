{
  JsonInputGSONAdapter jsonInputAdapter=new JsonInputGSONAdapter(propertyView,config.getDefaultIdProperty());
  ResultGSONAdapter resultGsonAdapter=new ResultGSONAdapter(propertyView,config.getDefaultIdProperty(),outputNestingDepth);
  return new GsonBuilder().setPrettyPrinting().serializeNulls().registerTypeHierarchyAdapter(FrameworkException.class,new FrameworkExceptionGSONAdapter()).registerTypeAdapter(JsonInput.class,jsonInputAdapter).registerTypeAdapter(Result.class,resultGsonAdapter).create();
}
