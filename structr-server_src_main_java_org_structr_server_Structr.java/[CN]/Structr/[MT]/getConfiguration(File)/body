{
  Properties props=new Properties();
  FileInputStream fis=null;
  try {
    fis=new FileInputStream(confFile);
    props.load(fis);
  }
 catch (  IOException ioex) {
    logger.log(Level.WARNING,"Unable to load settings from structr.conf");
  }
 finally {
    if (fis != null) {
      try {
        fis.close();
      }
 catch (      Throwable t) {
      }
    }
  }
  return props;
}
