{
  final NonFunctionHint hint=new NonFunctionHint(){
    @Override public String shortDescription(){
      return description;
    }
    @Override public String getSignature(){
      return signature;
    }
    @Override public String getName(){
      return name;
    }
  }
;
  if (replacement != null) {
    hint.setReplacement(replacement);
  }
  return hint;
}
