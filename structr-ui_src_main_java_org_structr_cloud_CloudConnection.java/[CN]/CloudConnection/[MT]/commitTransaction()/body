{
  if (tx != null) {
    try {
      if (CloudService.DEBUG) {
        System.out.println(System.currentTimeMillis() + " ############################### COMMITING TRANSACTION " + tx+ " in Thread"+ Thread.currentThread());
      }
      tx.success();
    }
 catch (    Throwable t) {
      t.printStackTrace();
    }
  }
 else {
    System.out.println("NO TRANSACTION!");
  }
}
