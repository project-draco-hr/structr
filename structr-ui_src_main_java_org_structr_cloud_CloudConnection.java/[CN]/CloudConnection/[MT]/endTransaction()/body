{
  if (tx != null) {
    if (CloudService.DEBUG) {
      System.out.println("############################### CLOSING TRANSACTION " + tx + " in Thread"+ Thread.currentThread());
    }
    try {
      tx.close();
    }
 catch (    Throwable t) {
      t.printStackTrace();
    }
    tx=null;
  }
  data.clear();
}
