{
  while (isConnected()) {
    try {
      final Message message=outputQueue.poll();
      if (message != null) {
        if (CloudService.DEBUG) {
          System.out.println("Sender: " + message);
        }
        outputStream.writeObject(message);
        outputStream.flush();
      }
    }
 catch (    IOException ioex) {
      errorMessage=ioex;
      finish();
    }
  }
}
