{
  final int size=source.size();
  int skip=0;
  int from=0;
  int to=size;
  if (skipCount != null) {
    skip=skipCount.intValue();
  }
  if (maxItems != null) {
    to=Math.min(maxItems.intValue(),size);
  }
  from=Math.min(skip,size);
  to=Math.min(to + skip,size);
  return source.subList(from,to);
}
