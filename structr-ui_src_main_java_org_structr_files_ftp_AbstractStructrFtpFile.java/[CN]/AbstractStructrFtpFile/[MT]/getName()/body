{
  try (Tx tx=StructrApp.getInstance().tx()){
    String name=structrFile != null ? structrFile.getProperty(File.name) : (newPath.contains("/") ? StringUtils.substringAfterLast(newPath,"/") : newPath);
    return name == null ? structrFile.getUuid() : name;
  }
 catch (  FrameworkException fex) {
    logger.log(Level.SEVERE,"Error in getName() of abstract ftp file",fex);
  }
  return null;
}
