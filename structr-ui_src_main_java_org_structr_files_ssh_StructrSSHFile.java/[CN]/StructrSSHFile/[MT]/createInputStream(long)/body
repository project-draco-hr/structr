{
  final App app=StructrApp.getInstance(parent.getSecurityContext());
  InputStream is=null;
  try (final Tx tx=app.tx()){
    if (actualFile != null) {
      is=((FileBase)actualFile).getInputStream();
    }
    tx.success();
  }
 catch (  FrameworkException fex) {
    logger.log(Level.WARNING,"",fex);
  }
  return is;
}
