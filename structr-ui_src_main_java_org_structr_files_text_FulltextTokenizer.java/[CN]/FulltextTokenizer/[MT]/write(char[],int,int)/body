{
  final int limit=off + len;
  final int length=Math.min(limit,cbuf.length);
  for (int i=off; i < length; i++) {
    final char c=cbuf[i];
    if (!Character.isAlphabetic(c) && !Character.isDigit(c) && !SpecialChars.contains(c)) {
      flush();
    }
 else {
      wordBuffer.append(c);
    }
  }
  rawText.append(new String(cbuf,off,len));
}
