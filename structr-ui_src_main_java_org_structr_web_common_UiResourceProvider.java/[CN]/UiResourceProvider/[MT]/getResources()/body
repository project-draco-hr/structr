{
  Map<Pattern,Class<? extends Resource>> resourceMap=new LinkedHashMap<>();
  resourceMap.put(Pattern.compile("[a-zA-Z0-9]{32}"),UuidResource.class);
  resourceMap.put(Pattern.compile("cypher"),CypherQueryResource.class);
  resourceMap.put(Pattern.compile("login"),LoginResource.class);
  resourceMap.put(Pattern.compile("logout"),LogoutResource.class);
  resourceMap.put(Pattern.compile("registration"),RegistrationResource.class);
  resourceMap.put(Pattern.compile("maintenance"),MaintenanceResource.class);
  resourceMap.put(Pattern.compile("in"),RelationshipResource.class);
  resourceMap.put(Pattern.compile("out"),RelationshipResource.class);
  resourceMap.put(Pattern.compile("start"),RelationshipNodeResource.class);
  resourceMap.put(Pattern.compile("end"),RelationshipNodeResource.class);
  resourceMap.put(Pattern.compile("public"),ViewFilterResource.class);
  resourceMap.put(Pattern.compile("protected"),ViewFilterResource.class);
  resourceMap.put(Pattern.compile("private"),ViewFilterResource.class);
  resourceMap.put(Pattern.compile("owner"),ViewFilterResource.class);
  resourceMap.put(Pattern.compile("admin"),ViewFilterResource.class);
  resourceMap.put(Pattern.compile("all"),ViewFilterResource.class);
  resourceMap.put(Pattern.compile("ids"),ViewFilterResource.class);
  resourceMap.put(Pattern.compile(PropertyView.Ui),ViewFilterResource.class);
  resourceMap.put(Pattern.compile(PropertyView.Html),ViewFilterResource.class);
  resourceMap.put(Pattern.compile("[a-zA-Z]+"),MaintenanceParameterResource.class);
  resourceMap.put(Pattern.compile("[0-9]+"),UuidResource.class);
  resourceMap.put(Pattern.compile("_schema"),SchemaResource.class);
  resourceMap.put(Pattern.compile("[a-z_A-Z]+"),TypeResource.class);
  return resourceMap;
}
