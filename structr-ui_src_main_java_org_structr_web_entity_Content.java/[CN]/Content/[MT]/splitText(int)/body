{
  checkWriteAccess();
  String text=getProperty(content);
  if (text != null) {
    int len=text.length();
    if (i < 0 || i > len) {
      throw new DOMException(DOMException.INDEX_SIZE_ERR,INDEX_SIZE_ERR_MESSAGE);
    }
