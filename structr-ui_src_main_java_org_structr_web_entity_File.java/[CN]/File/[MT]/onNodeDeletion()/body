{
  try {
    java.io.File toDelete=new java.io.File(getFileLocation().toURI());
    if (toDelete.exists() && toDelete.isFile()) {
      toDelete.delete();
    }
  }
 catch (  Throwable t) {
    logger.log(Level.WARNING,"Exception while trying to delete file {0}: {1}",new Object[]{getFileLocation(),t});
  }
}
