{
  if (AbstractNode.visibleToPublicUsers.equals(key) || AbstractNode.visibleToAuthenticatedUsers.equals(key) || AbstractNode.visibilityStartDate.equals(key)|| AbstractNode.visibilityEndDate.equals(key)|| AbstractNode.owner.equals(key)) {
    for (    Image tn : getThumbnails()) {
      tn.setProperty(key,value);
    }
  }
  return super.setProperty(key,value);
}
