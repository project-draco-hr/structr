{
  if (!renderContext.appLibRendered() && getProperty(new StringProperty(STRUCTR_ACTION_PROPERTY)) != null) {
    buffer.append(indent(depth)).append("<script type=\"text/javascript\" src=\"/structr/js/lib/jquery-1.10.2.min.js\"></script>").append(indent(depth)).append("<script type=\"text/javascript\" src=\"/structr/js/structr-app.js\"></script>");
    renderContext.setAppLibRendered(true);
  }
}
