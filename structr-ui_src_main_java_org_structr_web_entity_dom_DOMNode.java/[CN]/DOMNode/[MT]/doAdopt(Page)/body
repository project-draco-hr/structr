{
  if (_page != null) {
    final App app=StructrApp.getInstance(securityContext);
    try {
      app.beginTx();
      setProperty(ownerDocument,_page);
      app.commitTx();
    }
 catch (    FrameworkException fex) {
      throw new DOMException(DOMException.INVALID_STATE_ERR,fex.getMessage());
    }
 finally {
      app.finishTx();
    }
  }
  return this;
}
