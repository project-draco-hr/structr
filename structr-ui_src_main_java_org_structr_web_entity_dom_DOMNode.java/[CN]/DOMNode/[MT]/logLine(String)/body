{
  boolean doLog=false;
  if (this instanceof Page) {
    if (((Page)this).doDebugging()) {
      doLog=true;
    }
  }
 else {
    Page page=(Page)getOwnerDocument();
    if (page != null && page.doDebugging()) {
      doLog=true;
    }
  }
  if (doLog) {
    logger.log(Level.INFO,"################################################################################ {0}#{1}",new Object[]{getClass().getSimpleName(),method});
    Thread.dumpStack();
  }
}
