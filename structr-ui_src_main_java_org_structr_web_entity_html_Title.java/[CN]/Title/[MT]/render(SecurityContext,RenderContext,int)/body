{
  super.render(securityContext,renderContext,depth);
  if (RenderContext.EditMode.DATA.equals(renderContext.getEditMode())) {
    renderContext.getBuffer().append("\n    <script type=\"text/javascript\" src=\"/structr/js/lib/jquery-1.10.2.min.js\"></script>").append("\n    <script type=\"text/javascript\" src=\"/structr/js/structr-edit.js\"></script>").append("\n    <link rel=\"stylesheet\" type=\"text/css\" media=\"screen\" href=\"/structr/css/edit.css\"></link>");
  }
}
