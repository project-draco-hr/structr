{
  Map parameters=new HashMap<String,Object>();
  for (  Map.Entry<String,String[]> param : parameterMap.entrySet()) {
    String[] values=param.getValue();
    Object val;
    if (values.length == 1) {
      val=values[0];
    }
 else {
      val=values;
    }
    parameters.put(param.getKey(),val);
  }
  return parameters;
}
