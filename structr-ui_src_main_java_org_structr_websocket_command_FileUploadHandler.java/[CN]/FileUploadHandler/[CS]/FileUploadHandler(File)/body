{
  this.size=file.getProperty(File.size);
  this.file=file;
  if (this.size == null) {
    FileChannel channel;
    try {
      channel=getChannel();
      this.size=channel.size();
      updateSize(this.size);
    }
 catch (    IOException ex) {
      logger.log(Level.SEVERE,"Could not access file",ex);
    }
  }
}
