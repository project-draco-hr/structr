{
  if (reset) {
    RestAssured.given().contentType("application/json; charset=UTF-8").header("X-User","superadmin").header("X-Password","sehrgeheim").expect().statusCode(200).when().delete("/resource_access");
  }
  RestAssured.given().contentType("application/json; charset=UTF-8").header("X-User","superadmin").header("X-Password","sehrgeheim").body(" { 'signature' : '" + signature + "', 'flags': "+ flags+ " } ").expect().statusCode(201).when().post("/resource_access");
}
