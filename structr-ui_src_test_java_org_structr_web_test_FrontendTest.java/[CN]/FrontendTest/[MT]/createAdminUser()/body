{
  final PropertyMap properties=new PropertyMap();
  properties.put(User.name,"admin");
  properties.put(User.password,"admin");
  try {
    app.beginTx();
    User user=app.create(User.class,properties);
    user.setProperty(User.password,"admin");
    app.commitTx();
  }
 catch (  Throwable t) {
    t.printStackTrace();
  }
 finally {
    app.finishTx();
  }
}
