{
  List<LabelCacheItem> al=new ArrayList<LabelCacheItem>();
  for (  LabelCacheItem item : labelCache.values()) {
    if (isActive(item.getLayerIds()))     al.add(item);
  }
  for (  LabelCacheItem item : labelCacheNonGrouped) {
    if (isActive(item.getLayerIds()))     al.add(item);
  }
  return al;
}
