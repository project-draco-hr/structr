{
  if (rendererHints == null) {
    return optimizedDataLoadingEnabledDEFAULT;
  }
  Object result=null;
  try {
    result=rendererHints.get("optimizedDataLoadingEnabled");
  }
 catch (  ClassCastException e) {
  }
  if (result == null) {
    return optimizedDataLoadingEnabledDEFAULT;
  }
  return ((Boolean)result).booleanValue();
}
