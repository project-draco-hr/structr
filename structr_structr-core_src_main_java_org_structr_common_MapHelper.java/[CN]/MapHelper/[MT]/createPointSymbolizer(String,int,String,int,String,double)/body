{
  Mark mark=styleFactory.createMark();
  mark.setWellKnownName(filterFactory.literal(shape));
  mark.setStroke(styleFactory.createStroke(filterFactory.literal(strokeColor),filterFactory.literal(lineWidth)));
  mark.setFill(styleFactory.createFill(filterFactory.literal(fillColor),filterFactory.literal(fillOpacity)));
  Graphic graphic=styleFactory.createDefaultGraphic();
  graphic.graphicalSymbols().clear();
  graphic.graphicalSymbols().add(mark);
  graphic.setSize(filterFactory.literal(diam));
  PointSymbolizer sym=styleFactory.createPointSymbolizer(graphic,null);
  return sym;
}
