{
  boolean isAllowed=false;
switch (accessMode) {
case Backend:
    isAllowed=isAllowedInBackend(node,permission);
  break;
case Frontend:
isAllowed=isAllowedInFrontend(node,permission);
break;
}
if (node != null) {
User user=getUser();
logger.log(Level.FINEST,"Returning {0} for user {1}, access mode {2}, node {3}, permission {4}",new Object[]{isAllowed,(user != null) ? user.getName() : "null",accessMode,node.getId(),permission});
}
return isAllowed;
}
