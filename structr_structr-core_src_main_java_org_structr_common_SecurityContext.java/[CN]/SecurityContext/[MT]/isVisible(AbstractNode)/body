{
  boolean ret=false;
switch (accessMode) {
case Backend:
    ret=isVisibleInBackend(node);
  break;
case Frontend:
ret=isVisibleInFrontend(node);
break;
}
if (node != null) {
User user=getUser();
logger.log(Level.INFO,"Returning {0} for user {1}, access mode {2}, node {3}",new Object[]{ret,user != null ? user.getName() : "null",accessMode,node.getId()});
}
return (ret);
}
