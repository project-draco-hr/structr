{
  boolean visible=false;
switch (accessMode) {
case Backend:
    visible=isVisibleInBackend(node);
  break;
case Frontend:
visible=isVisibleInFrontend(node);
break;
}
if (visible) return visible;
if (node != null) {
user=getUser();
logger.log(Level.FINEST,"Returning {0} for user {1}, access mode {2}, node {3}",new Object[]{visible,(user != null) ? user.getName() : "null",accessMode,node});
}
return visible;
}
