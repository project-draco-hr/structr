{
  if (node == null) {
    return (false);
  }
  user=getUser();
  if (user == null) {
    return (false);
  }
  if (user instanceof SuperUser) {
    return true;
  }
  if (!user.isBackendUser()) {
    return (false);
  }
  if (isAllowedInBackend(node,Permission.Read)) {
    return (true);
  }
  return (false);
}
