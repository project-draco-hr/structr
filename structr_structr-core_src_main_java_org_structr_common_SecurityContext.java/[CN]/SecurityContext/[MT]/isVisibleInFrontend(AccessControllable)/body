{
  if (node == null) {
    return false;
  }
  if (node.isHidden()) {
    return false;
  }
  if (node.isVisibleToPublicUsers()) {
    return true;
  }
  if (node.isVisibleToAuthenticatedUsers()) {
    Principal user=getUser();
    if (user != null) {
      return true;
    }
  }
  return false;
}
