{
  if (o != null) {
    if (response != null) {
      try {
        response.getOutputStream().write(o.toString().getBytes(encoding));
      }
 catch (      Throwable t) {
        logger.log(Level.WARNING,"Exception while appending object to output stream: {0}",t);
      }
    }
 else     if (stringBuilder != null) {
      stringBuilder.append(o);
    }
  }
  return (this);
}
