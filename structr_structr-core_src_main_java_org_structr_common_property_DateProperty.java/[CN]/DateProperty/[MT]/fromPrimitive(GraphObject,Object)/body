{
  if (o != null) {
    if (o instanceof Long) {
      Date date=new Date();
      date.setTime((Long)o);
      return date;
    }
 else {
      try {
        return dateFormat.parse(o.toString());
      }
 catch (      Throwable t) {
        throw new FrameworkException(entity.getType(),new DateFormatToken(this));
      }
    }
  }
  return null;
}
