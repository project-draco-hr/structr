{
  String ret=null;
switch (path) {
case ConfigFile:
    ret=configFilePath;
  break;
case Base:
ret=basePath;
break;
case Database:
ret=getAbsolutePath(databasePath);
break;
case Files:
ret=getAbsolutePath(filesPath);
break;
case Modules:
ret=getAbsolutePath(modulesPath);
break;
case Temp:
ret=getAbsolutePath(tmpPath);
break;
}
return (ret);
}
