{
  if (source != null) {
    try {
      if (source instanceof Double) {
        return ((Double)source);
      }
 else       if (source instanceof String) {
        return NumberUtils.createDouble((String)source);
      }
    }
 catch (    Throwable t) {
      logger.log(Level.WARNING,"Exception while parsing double",t);
      return null;
    }
  }
  return source;
}
