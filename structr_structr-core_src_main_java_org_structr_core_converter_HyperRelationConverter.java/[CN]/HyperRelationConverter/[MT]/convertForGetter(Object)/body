{
  if (currentObject != null) {
    AbstractNode parent=(AbstractNode)currentObject;
    Notion notion=hyperRelation.getNotion();
    RelationClass parentRelation=new RelationClass(hyperRelation.getEntity(),hyperRelation.getRelType(),hyperRelation.getDirection(),RelationClass.Cardinality.ManyToMany,notion,0);
    return parentRelation.getRelatedNodes(securityContext,parent);
  }
  return null;
}
