{
  int count=0;
  if (currentObject != null) {
    Object toCount=currentObject.getProperty(propertyKey);
    if (toCount != null) {
      if (toCount instanceof Collection) {
        count=((Collection)toCount).size();
      }
 else       if (toCount instanceof Iterable) {
        for (        Object o : ((Iterable)toCount)) {
          count++;
        }
      }
 else {
        count=1;
      }
    }
  }
  return count;
}
