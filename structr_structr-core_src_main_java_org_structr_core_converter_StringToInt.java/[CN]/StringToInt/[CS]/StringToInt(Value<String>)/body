{
  super(source,new PropertyConverter<String,Integer>(null){
    @Override public String convertForSetter(    Integer source){
      if (source != null) {
        return source.toString();
      }
      return null;
    }
    @Override public Integer convertForGetter(    String source){
      if (source != null) {
        return Integer.parseInt(source);
      }
      return null;
    }
  }
);
}
