{
  if (iterable instanceof IterableAdapter) {
    IterableAdapter adapter=(IterableAdapter)iterable;
    if (adapter.size() >= 0) {
      return adapter.size();
    }
  }
  if (iterable instanceof Collection) {
    return ((Collection)iterable).size();
  }
  int count=0;
  for (  Object o : iterable) {
    count++;
  }
  return (count);
}
