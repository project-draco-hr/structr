{
  List<AbstractNode> nodes=new LinkedList<AbstractNode>();
  AbstractNode node=getParentNodeIgnorePermissions();
  do {
    nodes.add(node);
    node=node.getParentNodeIgnorePermissions();
  }
 while ((node != null) && !node.isRootNode());
  if (node != null) {
    nodes.add(node);
  }
  return nodes;
}
