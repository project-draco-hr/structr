{
  Object value=null;
  if (isDirty) {
    value=properties.get(key);
  }
  if ((key != null) && dbNode.hasProperty(key)) {
    value=dbNode.getProperty(key);
  }
  PropertyConverter converter=EntityContext.getPropertyConverter(this.getClass(),key);
  if (converter != null) {
    value=converter.convertTo(value);
  }
  return value;
}
