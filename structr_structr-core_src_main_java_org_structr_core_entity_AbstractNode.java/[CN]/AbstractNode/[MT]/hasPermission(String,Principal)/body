{
  if (principal == null || permission == null) {
    return false;
  }
  if (principal instanceof SuperUser) {
    return true;
  }
  if (this.equals(principal)) {
    return true;
  }
  StructrRelationship r=null;
  r=getSecurityRelationship(principal);
  if (r != null && r.isAllowed(permission)) {
    return true;
  }
  return false;
}
