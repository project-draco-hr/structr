{
  if (principal == null) {
    return false;
  }
  if (permission == null) {
    return false;
  }
  if (principal instanceof SuperUser) {
    return true;
  }
  if (this.equals(principal)) {
    return true;
  }
  AbstractRelationship r=getSecurityRelationship(principal);
  if ((r != null) && r.isAllowed(permission)) {
    return true;
  }
  return false;
}
