{
  Object oldValue=getProperty(key);
  if (oldValue == null && value == null) {
    return;
  }
  if (oldValue == null && value != null) {
    setPropertyInternal(key,value,updateIndex);
    return;
  }
  if (oldValue != null && value != null && !oldValue.equals(value)) {
    setPropertyInternal(key,value,updateIndex);
    return;
  }
}
