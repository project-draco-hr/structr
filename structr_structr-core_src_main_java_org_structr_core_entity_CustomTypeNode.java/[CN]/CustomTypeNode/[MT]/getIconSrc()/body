{
  if (typeNode == null) {
    typeNode=getTypeNode();
  }
  Object iconSrc=null;
  if (typeNode != null) {
    iconSrc=typeNode.getProperty(Key.iconSrc.name());
  }
  if ((iconSrc != null) && (iconSrc instanceof String)) {
    return (String)iconSrc;
  }
 else {
    return "/images/error.png";
  }
}
