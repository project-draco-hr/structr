{
  if (cardinality.equals(Cardinality.OneToMany) || cardinality.equals(Cardinality.ManyToMany)) {
    return getTraversalResults(securityContext,node,CaseHelper.toCamelCase(type));
  }
 else {
    logger.log(Level.WARNING,"Requested related nodes with wrong cardinality {0} between {1} and {2}",new Object[]{cardinality.name(),node.getClass().getSimpleName(),type});
  }
  return null;
}
