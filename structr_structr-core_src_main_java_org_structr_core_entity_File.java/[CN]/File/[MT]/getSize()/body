{
  long size=0L;
  Object obj=getProperty(SIZE_KEY);
  if (obj instanceof String) {
    try {
      size=Long.parseLong((String)obj);
      setProperty(SIZE_KEY,size);
    }
 catch (    NumberFormatException nfe) {
      logger.log(Level.SEVERE,"Could not parse {0} to long",obj);
    }
  }
  return size;
}
