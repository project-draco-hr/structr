{
  boolean error=false;
  for (  AbstractRelationship rel : this.getRelationships()) {
    AbstractNode otherNode=rel.getOtherNode(this);
    if (otherNode != null && otherNode instanceof Locatable) {
      error|=((Locatable)otherNode).locationChanged(securityContext);
    }
  }
  return error;
}
