{
  boolean value=false;
  if ("true".equalsIgnoreCase(required)) {
    value=true;
  }
 else   if ("yes".equalsIgnoreCase(required)) {
    value=true;
  }
 else   if ("ja".equalsIgnoreCase(required)) {
    value=true;
  }
 else   if ("1".equalsIgnoreCase(required)) {
    value=true;
  }
  this.required=value;
}
