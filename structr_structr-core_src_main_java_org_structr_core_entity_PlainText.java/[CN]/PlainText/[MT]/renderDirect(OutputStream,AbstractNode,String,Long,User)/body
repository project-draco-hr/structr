{
  if (isVisible(user)) {
    try {
      StringBuilder sb=new StringBuilder();
      renderView(sb,startNode,editUrl,editNodeId,user);
      IOUtils.write(sb.toString(),out);
    }
 catch (    IOException e) {
      logger.log(Level.WARNING,"Error while rendering {0}: {1}",new String[]{getContent(),e.getMessage()});
    }
  }
}
