{
  addDecorator(new Decorator<T>(){
    @Override public void decorate(    StructrNode t){
      t.init(graphDb.createNode());
    }
  }
);
  addDecorator(new Decorator<T>(){
    @Override public void decorate(    StructrNode t){
      if (t instanceof Activity) {
        ((Activity)t).setStartTimestamp(new Date());
      }
    }
  }
);
}
