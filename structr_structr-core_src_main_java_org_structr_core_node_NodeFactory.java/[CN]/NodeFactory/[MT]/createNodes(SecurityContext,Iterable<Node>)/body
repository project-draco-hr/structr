{
  List<AbstractNode> nodes=new LinkedList<AbstractNode>();
  if ((input != null) && input.iterator().hasNext()) {
    for (    Node node : input) {
      AbstractNode n=createNode(securityContext,node);
      if (n != null) {
        nodes.add(n);
      }
    }
  }
  return new Result(nodes,null,true,false);
}
