{
  Class nodeClass=(Class)Services.command(GetEntityClassCommand.class).execute(nodeType);
  AbstractNode ret=null;
  if (nodeClass != null) {
    try {
      ret=(AbstractNode)nodeClass.newInstance();
    }
 catch (    Throwable t) {
      ret=null;
    }
  }
  if (ret == null) {
    ret=new ArbitraryNode();
  }
  ret.init(node);
  ret.onNodeInstantiation();
  return ret;
}
