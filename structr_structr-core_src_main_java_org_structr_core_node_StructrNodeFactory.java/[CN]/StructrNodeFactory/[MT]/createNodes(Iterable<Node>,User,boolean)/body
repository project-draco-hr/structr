{
  List<AbstractNode> nodes=new ArrayList<AbstractNode>();
  if (input != null && input.iterator().hasNext()) {
    for (    Node node : input) {
      AbstractNode n=createNode(node);
      if ((user == null || n.readAllowed(user)) && (includeDeleted || !(n.isDeleted()))) {
        nodes.add(n);
      }
    }
  }
  return nodes;
}
