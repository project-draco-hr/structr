{
  if (target != null) {
    AbstractNode newLocation=(AbstractNode)Services.command(securityContext,FindNodeCommand.class).execute(securityContext.getUser(),currentNode,target);
    if (newLocation != null) {
      for (      Callback callback : callbacks) {
        callback.callback(newLocation.getId());
      }
      return (true);
    }
  }
 else {
    throw new NodeCommandException("No parameter");
  }
  return (false);
}
