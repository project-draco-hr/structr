{
  if (parameter instanceof Collection) {
    throw new InvalidParameterException("IN cannot handle multiple parmeters");
  }
 else {
    try {
      parentNodeId=Long.parseLong(parameter.toString());
    }
 catch (    Throwable t) {
      AbstractNode findNode=(AbstractNode)Services.command(securityContext,FindNodeCommand.class).execute(securityContext.getUser(),parameter);
      if (findNode != null) {
        parentNodeId=findNode.getId();
      }
 else {
        parentNodeId=-1;
      }
    }
    if (parentNodeId == -1) {
      throw new InvalidParameterException("Invalid parent node ID");
    }
  }
}
