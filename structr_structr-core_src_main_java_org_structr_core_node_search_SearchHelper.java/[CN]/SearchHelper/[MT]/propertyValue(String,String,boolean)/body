{
  if (strict) {
    return new TermQuery(new Term(key,value));
  }
 else {
    BooleanQuery query=new BooleanQuery();
    query.add(new TermQuery(new Term(key,value)),Occur.SHOULD);
    query.add(new WildcardQuery(new Term(key,"\"" + value + "\"")),Occur.SHOULD);
    query.add(new WildcardQuery(new Term(key,value + "*")),Occur.SHOULD);
    return (query);
  }
}
