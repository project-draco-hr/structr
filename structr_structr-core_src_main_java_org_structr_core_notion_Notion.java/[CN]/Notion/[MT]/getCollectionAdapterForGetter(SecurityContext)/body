{
  return new Adapter<List<S>,List<T>>(){
    @Override public List<T> adapt(    List<S> s) throws FrameworkException {
      List<T> list=new LinkedList<T>();
      for (      S o : s) {
        list.add(serializationStrategy.serialize(securityContext,type,o));
      }
      return list;
    }
  }
;
}
