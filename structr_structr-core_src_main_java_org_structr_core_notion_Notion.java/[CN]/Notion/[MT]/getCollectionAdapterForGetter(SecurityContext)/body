{
  return new Adapter<List<GraphObject>,List<T>>(){
    @Override public List<T> adapt(    List<GraphObject> s) throws FrameworkException {
      List<T> list=new LinkedList<T>();
      for (      GraphObject o : s) {
        list.add(serializationStrategy.serialize(securityContext,type,o));
      }
      return list;
    }
  }
;
}
