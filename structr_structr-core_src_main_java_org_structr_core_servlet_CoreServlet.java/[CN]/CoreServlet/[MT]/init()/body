{
  this.constraints=new LinkedHashMap<Matcher,Class>();
  this.gson=new GsonBuilder().setPrettyPrinting().registerTypeAdapter(AbstractNode.class,new AbstractNodeGSONAdapter()).create();
  constraints.put(Pattern.compile("[0-9]+").matcher(""),IdConstraint.class);
  constraints.put(Pattern.compile("[a-z]+").matcher(""),TypeConstraint.class);
}
