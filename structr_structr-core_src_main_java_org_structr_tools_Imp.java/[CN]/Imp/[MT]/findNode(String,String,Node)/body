{
  for (  Node n : Traversal.description().depthFirst().relationships(RelType.HAS_CHILD,Direction.OUTGOING).traverse(rootNode).nodes()) {
    if (n.hasProperty("type") && n.getProperty("type").equals(type) && n.hasProperty("name")&& n.getProperty("name").equals(name)) {
      return n;
    }
  }
  return null;
}
