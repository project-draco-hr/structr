{
  final PropertySet props=new PropertySet();
  props.put(AbstractNode.type,type);
  return transactionCommand.execute(new StructrTransaction<List<AbstractNode>>(){
    @Override public List<AbstractNode> execute() throws FrameworkException {
      List<AbstractNode> nodes=new LinkedList<AbstractNode>();
      for (int i=0; i < number; i++) {
        nodes.add(createNodeCommand.execute(props));
      }
      return nodes;
    }
  }
);
}
