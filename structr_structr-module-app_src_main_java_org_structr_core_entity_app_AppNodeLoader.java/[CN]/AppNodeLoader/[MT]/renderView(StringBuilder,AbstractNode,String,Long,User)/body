{
  String loaderSourceParameter=(String)getProperty(LOADER_SOURCE_PARAMETER_KEY);
  if (loaderSourceParameter != null) {
    Object value=getValue();
    out.append("<input type='hidden' name='");
    out.append(loaderSourceParameter);
    out.append("' value='");
    out.append(value);
    out.append("' />");
  }
}
