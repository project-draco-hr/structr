  ret.append("<img src=\"charts/areaChart\" />").append(lineSeparator);
  ret.append("<img src=\"charts/stackedAreaChart\" />").append(lineSeparator);
  ret.append("<img src=\"charts/lineChart\" />").append(lineSeparator);
  return (ret.toString());
}
