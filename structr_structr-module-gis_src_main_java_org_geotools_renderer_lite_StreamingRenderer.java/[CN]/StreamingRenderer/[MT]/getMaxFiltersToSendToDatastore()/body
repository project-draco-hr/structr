{
  try {
    if (rendererHints == null) {
      return defaultMaxFiltersToSendToDatastore;
    }
    Integer result=(Integer)rendererHints.get("maxFiltersToSendToDatastore");
    if (result == null) {
      return defaultMaxFiltersToSendToDatastore;
    }
    return result.intValue();
  }
 catch (  Exception e) {
    return defaultMaxFiltersToSendToDatastore;
  }
}
