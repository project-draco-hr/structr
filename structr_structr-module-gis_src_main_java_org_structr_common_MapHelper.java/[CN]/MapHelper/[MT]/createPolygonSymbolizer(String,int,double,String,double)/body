{
  org.geotools.styling.Stroke stroke=styleFactory.createStroke(filterFactory.literal(Color.decode(lineColor)),filterFactory.literal(lineWidth),filterFactory.literal(lineOpacity));
  Fill fill=styleFactory.createFill(filterFactory.literal(Color.decode(fillColor)),filterFactory.literal(fillOpacity));
  PolygonSymbolizer sym=styleFactory.createPolygonSymbolizer(stroke,fill,null);
  return sym;
}
