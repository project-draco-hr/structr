{
  try {
    if (isVisible()) {
      StringBuilder svgString=new StringBuilder();
      renderSVGMap(svgString,startNode);
      out.write(svgString.toString().getBytes());
    }
  }
 catch (  IOException e) {
    logger.log(Level.SEVERE,"Could not write SVG content to output stream: {0}",e.getStackTrace());
  }
}
