{
  if (editNodeId != null && getId() == editNodeId.longValue()) {
    renderEditFrame(out,editUrl);
  }
 else {
    if (isVisible()) {
      if (hasTemplate()) {
        template.setCallingNode(this);
        template.renderView(out,startNode,editUrl,editNodeId);
      }
 else {
        out.append(getName());
      }
    }
  }
}
