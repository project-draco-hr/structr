{
  if (editNodeId != null && getId() == editNodeId.longValue()) {
    renderEditFrame(out,editUrl);
  }
 else {
    if (isVisible(user)) {
      if (hasTemplate(user)) {
        template.setRequest(getRequest());
        template.setCallingNode(this);
        template.renderView(out,startNode,editUrl,editNodeId,user);
      }
 else {
        out.append(getName());
      }
    }
  }
}
