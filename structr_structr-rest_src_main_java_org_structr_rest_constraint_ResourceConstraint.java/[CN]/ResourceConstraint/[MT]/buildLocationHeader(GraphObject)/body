{
  StringBuilder uriBuilder=securityContext.getBaseURI();
  uriBuilder.append(getUriPart());
  uriBuilder.append("/");
  if (newObject != null) {
    if (idProperty == null) {
      uriBuilder.append(newObject.getId());
    }
 else {
      uriBuilder.append(newObject.getProperty(idProperty));
    }
  }
  return uriBuilder.toString();
}
