{
  if (resultCount > 0) {
    result.setResultCount(resultCount);
  }
  if (page > 0) {
    result.setPage(page);
  }
  if (pageSize > 0) {
    result.setPageSize(pageSize);
  }
  int pageCount=getPageCount(resultCount,pageSize);
  if (pageCount > 0) {
    result.setPageCount(pageCount);
  }
  return result;
}
