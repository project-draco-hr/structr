{
  this.securityContext=securityContext;
  this.request=request;
  this.rawType=part;
  if (rawType != null) {
    entityClass=EntityContext.getEntityClassForRawType(rawType);
    if (entityClass == null) {
      if (!EntityContext.isKnownProperty(new Property(part))) {
        return false;
      }
    }
  }
  return true;
}
