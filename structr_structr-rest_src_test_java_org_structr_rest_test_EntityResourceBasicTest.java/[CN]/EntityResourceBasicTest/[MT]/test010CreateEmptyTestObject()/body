{
  String location=RestAssured.given().contentType("application/json; charset=UTF-8").expect().statusCode(201).when().post("/test_objects").getHeader("Location");
  assertNotNull(location);
  String uuid=getUuidFromLocation(location);
  assertNotNull(uuid);
  assertFalse(uuid.isEmpty());
  assertTrue(uuid.matches("[a-f0-9]{32}"));
  Object name=RestAssured.given().contentType("application/json; charset=UTF-8").expect().statusCode(200).body("result_count",equalTo(1)).body("query_time",lessThan("0.1")).body("serialization_time",lessThan("0.001")).body("result.id",equalTo(uuid)).when().get("/test_objects/" + uuid).jsonPath().get("result.name");
  assertTrue(name instanceof JSONNull);
}
