{
  RestAssured.baseURI="http://server0.morgner.de";
  RestAssured.port=8888;
  RestAssured.basePath="/splink_sgdb/api";
  String res="/version";
  try {
    System.out.println("Testing " + res + "...");
    expect().body(containsString("0.2.8")).when().get(res);
  }
 catch (  Throwable t) {
    t.printStackTrace();
  }
}
