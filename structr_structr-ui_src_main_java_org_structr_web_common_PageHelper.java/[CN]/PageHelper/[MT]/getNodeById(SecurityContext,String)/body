{
  if (id == null) {
    return null;
  }
  try {
    return (AbstractNode)Services.command(securityContext,GetNodeByIdCommand.class).execute(id);
  }
 catch (  Throwable t) {
    logger.log(Level.WARNING,"Unable to load node with id {0}, {1}",new java.lang.Object[]{id,t.getMessage()});
  }
  return null;
}
