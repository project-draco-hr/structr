{
  List<NodeAttribute> attrs=new LinkedList<NodeAttribute>();
  for (  Map.Entry<String,String[]> param : parameterMap.entrySet()) {
    String[] values=param.getValue();
    Object val;
    if (values.length == 1) {
      val=values[0];
    }
 else {
      val=values;
    }
    NodeAttribute attr=new NodeAttribute(param.getKey(),val);
    attrs.add(attr);
  }
  return attrs;
}
